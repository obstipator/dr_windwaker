"LightmappedGeneric"
{
	"$basetexture" "zelda/water"
	"$alpha" 0.6
	"$normalmapalphaenvmapmask" "1"
	"$surfaceprop" "water"
	"$envmap" "env_cubemap"
	"$envmaptint" "[ 1.00 0.89 0.80 ]"
	"$envmapcontrast" "1"
	
	"$bottommaterial" "nature/toxicslime002a_beneath"

	"$offset" "0"
	"$rotate" "90.0"
	
	"Proxies"
	{
		"Sine"
		{
			"sineperiod" 50
			"sinemin" 0
			"sinemax" 45
			"timeoffset" "$offset"
			"resultVar" "$rotate"
		}

		"TextureScroll"
		{
			"textureScrollVar" "$basetexturetransform"
			"textureScrollRate" 0.1
			"textureScrollAngle" "$rotate"
		}
	}
}



